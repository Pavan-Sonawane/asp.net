@model List<Course>

<h2>Enrolled Courses</h2>

@if (Model != null && Model.Any())
{
    <table class="table">
        <thead>
            <tr>
                <th>Course ID</th>
                <th>Course Name</th>
                
            </tr>
        </thead>
        <tbody>
            @foreach (var course in Model)
            {
                <tr>
                    <td>@course.Course_Id</td>
                    <td>@course.Course_Name</td>
                  
                </tr>
            }
        </tbody>
    </table>
}
else
{
    <p>No enrolled courses found for the provided first name.</p>
}
