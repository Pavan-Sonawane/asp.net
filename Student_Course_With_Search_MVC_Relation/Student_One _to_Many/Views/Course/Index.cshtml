@model IEnumerable<Student_One__to_Many.Models.Course>

<p>
    <a asp-action="Add">Create New</a>
</p>
<table class="table">
    <thead>
        <tr>
            <th>
                @Html.DisplayNameFor(model => model.CourseName)
            </th>
            <th></th>
        </tr>
    </thead>
    <tbody>
@foreach (var item in Model) {
        <tr>
            <td>
                @Html.DisplayFor(modelItem => item.CourseName)
            </td>
            <td>
                <a asp-action="Edit" asp-route-id="@item.Course_Id">Edit</a> |
                <a asp-action="Details" asp-route-id="@item.Course_Id">Details</a> |
                <a asp-action="Delete" asp-route-id="@item.Course_Id">Delete</a>
            </td>
        </tr>
}
    </tbody>
</table>
